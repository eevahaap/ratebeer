<p id="notice"><%= notice %></p>

<h2>List of ratings</h2>
<p>All ratings: <%= @ratings.count %></p>

<ul>
  <% @ratings.each do |rating| %>
  <li>
    <%= rating.beer.name %>
    <%= rating.score %>
    <%= link_to rating.user.username, rating.user %>
  </li>

  <% end %>
</ul>

<%= link_to 'Add new rating', new_rating_path%>

